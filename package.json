    "lint-staged": {
        "ignore": [
            "package.json"
        ],
        "linters": {
            "*.{js,jsx}": [
                "npm run fix-js",
                "git add --force"
            ],
            "*.{json,md,graphql}": [
                "prettier --write",
                "git add --force"
            ],
            "*.{css,less,styl,scss,sass,sss}": [
                "stylelint --fix",
                "git add --force"
            ]
        }
    },
    "scripts": {
        "precommit": "lint-staged",
        "lint-js": "eslint --ext .ts --ext .tsx --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
        "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
        "lint": "yarn run lint-js && yarn run lint-css",
        "fix-js": "yarn run lint-js --fix",
        "fix-css": "yarn run lint-css --fix",
        "fix": "yarn run fix-js && yarn run fix-css",
        "test": "tsc && jest",
        "test-watch": "yarn run test --watch --notify",
        "test-cover": "yarn run test --coverage",
        "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
        "clean": "ts-node --project tsconfig.node.json tools/run clean",
        "copy": "ts-node --project tsconfig.node.json tools/run copy",
        "bundle": "ts-node --project tsconfig.node.json tools/run bundle",
        "build": "ts-node --project tsconfig.node.json tools/run build",
        "build-stats": "yarn run build --release --analyse",
        "deploy": "ts-node --project tsconfig.node.json tools/run deploy",
        "render": "ts-node --project tsconfig.node.json tools/run render",
        "serve": "ts-node --project tsconfig.node.json tools/run runServer",
        "start": "ts-node --project tsconfig.node.json tools/run start",
        "codegen": "graphql-codegen --config codegen.yml",
        "codegen-watch": "graphql-codegen --config codegen.yml --watch"
    }
